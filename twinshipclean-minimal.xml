		    },
		    {
		      title: "Theme Color",
		      value: userProfile.accentColor.replace('neon-', '').charAt(0).toUpperCase() + 
		             userProfile.accentColor.replace('neon-', '').slice(1),
		      editStep: 4,
		      icon: "color-palette" as const,
		    },
		  ];
		
