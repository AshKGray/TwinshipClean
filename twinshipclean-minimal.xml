		                  <Ionicons name="flask" size={20} color="white" />
		                  <Text className="text-white ml-3">Twinquiry Participation</Text>
		                </View>
		                <Ionicons 
		                  name={researchParticipation ? "toggle" : "toggle-outline"} 
