		      };
		      
		      expect(transformLikertTo100Scale(1, customConfig)).toBe(0);
		      expect(transformLikertTo100Scale(3, customConfig)).toBe(50);
		      expect(transformLikertTo100Scale(5, customConfig)).toBe(100);
