		      emotionalRatings: {
		        joy: 0,
		        sadness: 0,
		        peace: 0,
		        anxiety: 0,
		        love: 0,
		        curiosity: 0
		      }
		    });
		    setSelectedWords([]);
		    
		    // Auto-advance after viewing time
		    setTimeout(() => {
		      setGamePhase('rating');
